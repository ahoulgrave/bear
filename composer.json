{
    "name": "ahoulgrave/bear",
    "description": "Bear Component",
    "authors": [
        {
            "name": "Agustin Houlgrave",
            "email": "a.houlgrave@gmail.com"
        }
    ],
    "require": {
        "symfony/http-foundation": "^3.2",
        "symfony/event-dispatcher": "^3.2",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {"Bear\\": "src/Bear"}
    },
    "autoload-dev": {
        "psr-4": {"Bear\\Tests\\": "tests/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "license": "MIT"
}
